<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.adgexternals.homefragment"
        android:label="Home"
        tools:layout="@layout/fragment_homefragment" />

    <fragment
        android:id="@+id/navigation_recruitment"
        android:name="com.example.adgexternals.recruitmentfragment"
        android:label="Recruitments"
        tools:layout="@layout/fragment_recruitmentfragment" />

    <fragment
        android:id="@+id/navigation_faq"
        android:name="com.example.adgexternals.faq_fragment"
        android:label="F.A.Q"
        tools:layout="@layout/fragment_faq_fragment" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.adgexternals.settings_fragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings_fragment" />

</navigation>